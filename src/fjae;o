#include <iostream>
#include <cstdio>
#include <bitset>
using namespace std;
int main(void){
    unsigned long long int n;
    scanf("%lld",&n);
    if(n<2) {
        printf("1\n");
        return 0;
    } else if(n%2==1) {
        printf("0\n");
        return 0;
    }
    unsigned long long int c=0;
    printf("%lld\n",n);
    while(n%10==0){
        c += n/10;
        n = n/10;
        
        printf("%lld %lld\n",n, c);
    }
    printf("%lld\n",c);
    return 0;
}
